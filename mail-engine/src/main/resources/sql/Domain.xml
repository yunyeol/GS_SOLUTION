<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="SQL.Domain">

    <select id="selectDomainList" resultType="Domain">
        SELECT
            DOMAIN domain,
            SCORE score,
            REG_DATE regDate
        FROM GS_DOMAIN_LIST
    </select>
</mapper>

