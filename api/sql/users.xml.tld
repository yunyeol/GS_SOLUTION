<?xml version="1.0" encoding="UTF-8" ?>
<query>
    <!-- select Query -->
    <selectUsers>
        SELECT
            GM.LOGIN_ID,
            GM.MBR_NAME,
            GM.ACTIVE_YN,
            GG.GRP_NAME,
            GA.AUTH_NAME,
            GM.REG_DATE
        FROM GS_MBR GM
        INNER JOIN GS_GRP GG ON (GM.MBR_GRP_ID = GG.MBR_GRP_ID)
        INNER JOIN GS_AUTH GA ON (GM.MBR_AUTH_ID = GA.MBR_AUTH_ID)
        WHERE 1=1
    </selectUsers>

</query>
