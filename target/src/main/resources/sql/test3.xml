<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="master.slave.test">

    <select id="selectData" resultType="Map">
        SELECT MIN(ID) MIN_ID,
                MAX(ID) MAX_ID
        FROM test
    </select>

    <select id="selectData2" resultType="HashMap" parameterType="Map">
        SELECT
              TEST
        FROM test
        WHERE ID BETWEEN #{minValue} AND #{maxValue}
    </select>

    <update id="insertData">
        INSERT INTO TEST(TEST) VALUES (22)
    </update>

</mapper>

